	"description": "An implementation of Express, PassportJs and OIDC-Provider to provide a reusable and extendable OIDC Provider",
	"main": "server.js",
	"scripts": {
		"start": "set DEPLOYMENT_ENV=local && nodemon server",
		"test": "mocha --reporter dot",
		"coverage": "nyc --reporter html --reporter text npm test"
	},
	"keywords": [
		"OIDC",
		"Express",
		"PassportJS",
		"OIDC-Provider",
